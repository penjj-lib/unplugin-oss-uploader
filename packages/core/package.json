{
  "name": "unplugin-oss-uploader",
  "type": "module",
  "version": "0.0.0-alpha.2",
  "description": "Upload assets to OSS with Vite or Webpack",
  "author": "penjj<peng.j.1995@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/penjj/unplugin-oss-uploader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penjj/unplugin-oss-uploader.git",
    "directory": "packages/unplugin-oss-uploader"
  },
  "bugs": {
    "url": "https://github.com/penjj/unplugin-oss-uploader/issues"
  },
  "keywords": [
    "vite",
    "webpack",
    "oss",
    "qiniu",
    "aliyun",
    "taro",
    "wechat miniprogram",
    "tencent",
    "oss",
    "cdn",
    "unplugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./adapter": {
      "types": "./dist/adapter.d.ts",
      "import": "./dist/adapter.mjs",
      "require": "./dist/adapter.cjs"
    },
    "./asset-loader": {
      "types": "./dist/asset-loader.d.ts",
      "import": "./dist/asset-loader.mjs",
      "require": "./dist/asset-loader.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "cos-nodejs-sdk-v5": ">=2",
    "postcss": "*",
    "qiniu": ">=7",
    "unplugin": ">=1",
    "vite": "*",
    "webpack": ">=4"
  },
  "peerDependenciesMeta": {
    "cos-nodejs-sdk-v5": {
      "optional": true
    },
    "postcss": {
      "optional": true
    },
    "qiniu": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "chalk": "^5.3.0",
    "cos-nodejs-sdk-v5": "^2.12.6",
    "postcss": "^8.4.33",
    "qiniu": "^7.11.0",
    "unplugin": "^1.6.0",
    "vite": "^5.0.12",
    "webpack": "^5.90.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "unbuild": "^2.0.0"
  }
}
